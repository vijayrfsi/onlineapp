{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "repositories": {
        "0": {
            "type": "path",
            "url": "/Applications/MAMP/htdocs/vidy/setup/packages/jtinkers/borgcms/"
        },
        "vijay": {
            "type": "path",
            "url": "packages/vijay"
        }
    },
    "require": {
        "php": "^8.0.2",
        "dcblogdev/laravel-module-generator": "^1.0",
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "^4.0",
        "intervention/image": "^2.4",
        "kitloong/laravel-migrations-generator": "^6.11",
        "laracademy/generators": "^3.7",
        "laravel/framework": "^9.19",
        "laravel/sanctum": "^3.3",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^3.4",
        "laravelcollective/html": "*",
        "nwidart/laravel-modules": "^8.3",
        "silber/bouncer": "*",
        "vijayass/vijay": "dev-main"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^1.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "~8.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "aliases": {
                "Vijay": "App\\VijayFacade"
            }
        }
    }
}
