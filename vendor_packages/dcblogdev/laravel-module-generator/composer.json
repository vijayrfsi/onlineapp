{
    "name": "dcblogdev/laravel-module-generator",
    "description": "Generate Laravel Modules from a template.",
    "license": "MIT",
    "authors": [
        {
            "name": "David Carr",
            "email": "dave@dcblog.dev",
            "homepage": "https://dcblog.dev"
        }
    ],
    "homepage": "https://github.com/dcblogdev/laravel-module-generator",
    "keywords": ["Laravel", "Laravel Modules", "Generator"],
    "require": {
        "illuminate/support": "8.x|9.x|10.x",
        "symfony/filesystem": "^5.0|^6.0",
        "symfony/finder": "^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Dcblogdev\\ModuleGenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dcblogdev\\ModuleGenerator\\ModuleGeneratorServiceProvider"
            ],
            "aliases": {
                "Generator": "Dcblogdev\\ModuleGenerator\\Facades\\ModuleGenerator"
            }
        }
    }
}
